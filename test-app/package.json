{
  "name": "test-app",
  "version": "1.0.0",
  "description": "This is a test application for flushing out framework use cases",
  "private": true,
  "scripts": {
    "clean": "rimraf build",
    "client:dev": "CLIENT_PORT=$(grep CLIENT_PORT .env | cut -d '=' -f2) SERVER_PORT=$(grep SERVER_PORT .env | cut -d '=' -f2) webpack serve --config webpack.client.js",
    "server:dev": "webpack --config webpack.server.js --watch",
    "server:test": "run-s clean server:dev",
    "both:dev": "run-p server:dev client:dev",
    "dev": "run-s clean both:dev"
  },
  "keywords": [],
