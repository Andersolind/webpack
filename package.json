{
  "name": "indigo-js",
  "version": "0.0.0",
  "description": "IndigoJS framework",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://artifactory.indigint.net/artifactory/api/npm/npm/"
  },
  "scripts": {
    "dev": "nodemon index.js",
    "start:client": "webpack-dev-server --config ./webpack/webpack.client.dev.js --mode development",
    "build:client": "webpack --config ./webpack/webpack.client.prod.js --mode=production",
    "build:server": "webpack --config ./webpack/webpack.server.prod.js",
    "start:server": "node dist/server.js",
    "dev:server": "webpack --config ./webpack/webpack.server.dev.js && node dist/server.js",
    "prod:server": "webpack --config ./webpack/webpack.server.prod.js && node dist/server.js",
    "lint": "eslint .",
    "test": "jest . --coverage"
  },
  "author": "eComm",
  "license": "ISC",
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-proposal-object-rest-spread"
    ]
  },
  "dependencies": {
    "core-js": "^3.18.3",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "history": "^5.0.0",
    "install-deps": "^1.1.0",
    "isomorphic-fetch": "^3.0.0",
    "morgan": "^1.10.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.0.0-beta.0",
    "regenerator-runtime": "^0.13.9",
    "serialize-javascript": "^6.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@babel/plugin-proposal-object-rest-spread": "^7.14.7",
    "@babel/plugin-transform-runtime": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "@babel/preset-react": "^7.14.5",
    "@testing-library/react": "^12.1.2",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "css-loader": "^5.1.3",
    "eslint": "^7.32.0",
    "eslint-config-indigo": "^1.3.1",
    "eslint-config-indigo-react": "^1.0.2",
    "eslint-plugin-import": "^2.24.0",
    "eslint-plugin-security": "^1.4.0",
    "file-loader": "^6.2.0",
    "html-loader": "^1.3.2",
    "html-webpack-plugin": "^5.3.1",
    "jest": "^27.3.1",
    "jest-playwright-jsdom": "^0.0.4",
    "mini-css-extract-plugin": "^2.0.0",
    "node-sass": "^5.0.0",
    "nodemon": "^2.0.13",
    "playwright": "^1.15.2",
    "react-test-renderer": "^17.0.2",
    "sass-loader": "^10.1.0",
    "style-loader": "^2.0.0",
    "supertest": "^6.1.6",
    "webpack": "^5.58.2",
    "webpack-cli": "^4.9.0",
    "webpack-dev-server": "^4.3.1",
    "webpack-node-externals": "^3.0.0"
  }
}
